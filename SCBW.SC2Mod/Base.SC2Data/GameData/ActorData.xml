        <LaunchAttachQuery Methods="AMFilterWeaponLeft"/>
        <LaunchAssets Sound="ZealotSCBW_AttackLaunch"/>
        <ImpactMap index="None" Sound="ZealotSCBW_AttackImpact"/>
        <ImpactPhysics>
            <Name value="Attack00"/>
            <MatchKeys value="Basic"/>
            <AttackAnimProps value="00"/>
            <Physics value="ZealotSCBWAttackDeathForce00"/>
        </ImpactPhysics>
        <ImpactPhysics>
            <Name value="Attack01"/>
            <MatchKeys value="Basic"/>
            <AttackAnimProps value="01"/>
            <Physics value="ZealotSCBWAttackDeathForce01"/>
        </ImpactPhysics>
        <ImpactPhysics>
            <Name value="Attack02"/>
            <MatchKeys value="Basic"/>
            <AttackAnimProps value="02"/>
            <Physics value="ZealotSCBWAttackDeathForce02"/>
        </ImpactPhysics>
        <AcquisitionYawHalfArc value="25.000000"/>
        <AcquisitionPitchHalfArc value="25.000000"/>
        <ShieldFlashType value="Header"/>
