        <Receiver Subject="::Main"/>
        <Field value="Radial"/>
        <Duration value="0.150000"/>
        <Magnitude value="27.500000,15.000000"/>
        <HostEnd Subject="::Self"/>
        <HostEndSiteOps Ops="SOpPointUpWorld SOpMoveBy1Dot5"/>
