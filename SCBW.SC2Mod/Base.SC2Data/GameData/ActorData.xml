    <CActorForceSphere id="ZealotSCBWAttackDeathForce00">
        <Host Subject="::Main" Scope="Target"/>
        <HostSiteOps Ops="SOpAttachCenter SOpForwardCasterCenter SOpMoveByDot15 SOpRandomPointInCrossbarDot25GaussInv"/>
